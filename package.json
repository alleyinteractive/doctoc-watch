{
  "name": "doctoc-watch",
  "version": "0.0.1",
  "description": "Doctoc Watcher written in node",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/babel-node ./src/doctoc-watch.js -t ./test/README.md -T '#Table of Contents' -l '**/*.md','!**/node_modules/**/*.md' -L '##Random Other Title'",
    "test-once": "./node_modules/.bin/babel-node ./src/doctoc-watch.js -t ./test/README.md -T '#Table of Contents' -l '**/*.md','!**/node_modules/**/*.md' -L '##Random Other Title' -r",
    "prepublish": "./node_modules/.bin/babel -d lib src/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alleyinteractive/doctoc-watch.git"
  },
  "keywords": [
    "doctoc",
    "doc",
    "watcher",
    "doctoc-watcher"
  ],
  "bin": {
    "doctoc-watch": "src/doctoc-watch.js"
  },
  "author": "Alley Interactive",
  "license": "GPL v2",
  "bugs": {
    "url": "https://github.com/alleyinteractive/doctoc-watch/issues"
  },
  "homepage": "https://github.com/alleyinteractive/doctoc-watch#readme",
  "dependencies": {
    "commander": "^2.9.0",
    "doctoc": "^0.15.0",
    "gaze": "^0.5.2"
  },
  "devDependencies": {
    "babel-cli": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "jasmine-node": "^1.14.5",
    "rewire": "^2.5.1"
  }
}
